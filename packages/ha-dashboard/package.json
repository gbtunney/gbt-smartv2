{
    "name": "@gbt/ha-dashboard",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "\n========== DEVELOPMENT >> ==========": "",
        "dev": "pnpm build:ts --watch",
        "dev:ts": "pnpm build:ts --watch",
        "dev:rollup": "rollup --watch --config ./rollup.config.js",
        "dev:vite": "pnpm exec vite --config ./vite.config.ts",
        "\n========== BUILD >> ==========": "",
        "build": "pnpm build:ts && pnpm build:vite",
        "build:ts": "pnpm exec tsc --build",
        "build:rollup": "rollup --config ./rollup.config.js",
        "build:vite": "pnpm exec vite build --config ./vite.config.ts",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:ts && pnpm clean:build",
        "clean:ts": "pnpm exec tsc --build --clean",
        "clean:build": "pnpm exec rm -rf ./dist ./types",
        "\n========== TEST >> ==========": "",
        "test": "echo '\"\\n===\\n>> Unit Test:: no test specified!'",
        "\n========== PUBLISH >> ==========": "",
        "sync": "pnpm exec tsx ./sync-types.ts",
        "deploy": "pnpm build && pnpm exec tsx ./scripts/deploy.ts",
        "\n========== MISC >> ==========": "",
        "lint": "eslint .",
        "preview": "vite preview"
    },
    "dependencies": {
        "@hakit/components": "^5.0.2",
        "@hakit/core": "^5.0.2",
        "@mui/icons-material": "^6.4.4",
        "@mui/material": "^6.4.4",
        "@mui/x-charts": "^7.26.0",
        "@mui/x-data-grid": "^7.26.0",
        "@mui/x-date-pickers": "^7.26.0",
        "@mui/x-tree-view": "^7.26.0",
        "@rjsf/core": "^5.24.3",
        "@rjsf/mui": "^5.24.3",
        "@rjsf/utils": "^5.24.3",
        "@rjsf/validator-ajv8": "^5.24.3",
        "@snailicide/g-library": "^1.2.0",
        "@types/json-schema": "^7.0.15",
        "@types/ramda": "^0.30.2",
        "home-assistant-js-websocket": "^9.4.0",
        "json-schema": "^0.4.0",
        "l": "^0.6.0",
        "minimatch": "^10.0.1",
        "ramda": "^0.30.1",
        "ramda-adjunct": "^5.1.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.19.0",
        "@snailicide/build-config": "^1.4.0",
        "@types/node": "^22.13.4",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@unocss/preset-uno": "^65.4.3",
        "@vitejs/plugin-react": "^4.3.4",
        "chalk": "^5.4.1",
        "dotenv": "^16.4.7",
        "eslint": "^9.19.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.18",
        "globals": "^15.14.0",
        "node-scp": "^0.0.23",
        "tsx": "^4.19.2",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.22.0",
        "unocss": "^65.4.3",
        "vite": "^6.1.0",
        "zod-to-json-schema": "^3.24.1"
    }
}
